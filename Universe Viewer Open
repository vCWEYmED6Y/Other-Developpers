	TextColor3 = uiColor,
	TextTransparency = .3, 
	RichText = true, 
	TextScaled = true, 
	Name = "PlayerTitle",
})
local playerDescription = other("TextLabel", frameInfoDesc, {
	BackgroundTransparency = 1, 
