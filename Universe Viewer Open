--[[
Made by frog
      Universe Viewer V2 allows you to join any roblox experience linked to your current experience's universe.
      Left Click = Teleport to experience with timer (Will abort if failed or if teleportAbortDelay has been reached (Teleport may still work once delay is reached.))
you cannot teleport to another experience while teleport timer hasn't reached teleportAbortDelay or if teleport hasn't failed.

      Right Click = Copy experience ID to your clipboard, works when you are teleporting and shows experience's ID for 1.5s.
      
      Values you can edit: Every (avoid editing some unless you know what you are doing)
      Values useful to edit: 
          colors (don't remove/edit default ones), 
          uiColor (default = Pink),
          teleportAbortDelay (default = math.huge),
          particlesEnabled (default = true),
          particleAmount (default = 20),
          particleSize (default = UDim2.new(0,10,0,10),
	  getrenv().GameInfos = true -- TOGGLE TO TRUE TO VIEW GAME INFOS, [!!] WARNING [!!] LOTS OF DATA LEAKS + Longer Load (only once dw)


/// LOADSTRING: ///

getrenv().GameInfos = true 
getrenv().Debug = true 
getrenv().MobileFix = true
local link = "https://raw.githubusercontent.com/vCWEYmED6Y/Universal/main/Universe%20Viewer%20V2.lua"

local output, results = pcall(function()
	loadstring(game:HttpGet(link))()
end)

if not output then 
		print(results)

		if getrenv().ScreenGui then getrenv().ScreenGui:Destroy() end

		getrenv().ScreenGui = nil 
		getrenv().UniverseViewerDestroy = nil
		getrenv().UniverseViewerExecuted = false

end 

///   ///
]]--


-- Anti "Double Execute"
if getrenv().UniverseViewerExecuted then 

	return false
end 
getrenv().UniverseViewerExecuted = true 

-- Variables
local colors = {
	White = Color3.fromRGB(255,255,255),
	Black = Color3.fromRGB(0,0,0),
	Red = Color3.fromRGB(255,0,0),
	Green = Color3.fromRGB(0,255,0),
	Blue = Color3.fromRGB(0,0,255),
	Pink = Color3.fromRGB(255, 85, 255),
	DarkGrey = Color3.fromRGB(34,34,34),
	LightGrey = Color3.fromRGB(190,190,190),

}; local uiColor = colors.Pink
local guiParent = game.CoreGui
local teleportAbortDelay = math.huge
local particlesEnabled, particleAmount, particleSize = false, 20, UDim2.new(0,10,0,10) -- you don't have to edit particleAmount if particlesEnabled is set to true.
local particleModule = {}

getrenv().GameInfos = true 
getrenv().Debug = true 

-- Consts
local defaultValues = {
	["Enabled"] = false, 
	["Name"] = "",
	["ResetOnSpawn"] = false,
};
local defaultParticleProperties = {
	["ZIndex"] = 99999999,
	["Size"] = particleSize,
	["AnchorPoint"] = Vector2.new(.5,.5),
	["SizeConstraint"] = Enum.SizeConstraint.RelativeYY, 
	["BorderSizePixel"] = 0, 
	["Image"] = "http://www.roblox.com/asset/?id=184442073",
	["BackgroundTransparency"] = 1,
	["ImageTransparency"] = .5
};local blacklistedValues = {"Parent", "ZIndexBehavior", "DisplayOrder", "Archivable"}
local oldNames, lastClicked, currentText, index, teleportFailure, isTeleporting, addedValue = {}, {}, {}, {}, {}, false, 0
local stringColor = tostring(math.floor(uiColor.R*255)) .. "," ..tostring(math.floor(uiColor.G*255)) .. "," ..math.floor(tostring(uiColor.B*255))
local left = true
local gameDataStore = {}
lastClicked["ClipBoard"] = {}
lastClicked["Teleport"] = {}

-- Constants
local tpService = game:GetService("TeleportService")
local hService = game:GetService("HttpService")
local aService = game:GetService("AssetService")
local tService = game:GetService("TweenService")
local gService = game:GetService("GroupService")
local pages = aService:GetGamePlacesAsync()

-- Necessary Func
local function _print(...) 	if getrenv().Debug then  		print(...) 	end end
local function isProperty(instance, property)	local x = pcall(function()		return instance[property]	end)		return x end
local function isInstance(className)	local x = pcall(function()		return Instance.new(className)	end)		return x end
local function editUi(ui, parent,values)	if  type(values) ~= "table" then _print("\"values\" has to be a table.") return end	ui.Parent = parent	for i, v in pairs (values) do 		if not isProperty(ui, i) and not table.find(i, blacklistedValues) then _print("Could not set ".. tostring(i) .. " to " .. tostring(v)) continue end				ui[i] = v	end end
local function getPlaceDetails(placeId)	local link = `https://apis.roblox.com/universes/v1/places/{placeId}/universe`	local request = hService:JSONDecode(game:HttpGetAsync(link))		return request end
local function getGroupInfos(groupID)	return gService:GetGroupInfoAsync(groupID) end --[[ EmblemUrl, Id, Name, Owner (table: Id, Name), Roles (table)]]

-- UI Func
local function screenGui()	if not getrenv().ScreenGui then 		local newGui = Instance.new("ScreenGui", guiParent)		for i,v in pairs (defaultValues) do 			if not isProperty(newGui, i) and not table.find(i, blacklistedValues) then _print("Could not set ".. tostring(i) .. " to " .. tostring(v)) continue end						newGui[i] = v 		end				getrenv().ScreenGui = newGui				return newGui	end 		return getrenv().ScreenGui end
local function frame(parent, values)	if  type(values) ~= "table" then _print("\"values\" has to be a table.") return end	local newFrame = Instance.new("Frame", parent)		for i, v in pairs (values) do 		if not isProperty(newFrame, i) and not table.find(i, blacklistedValues) then _print("Could not set ".. tostring(i) .. " to " .. tostring(v)) continue end 				newFrame[i] = v 	end		return newFrame end
local function other(className, parent, values)	if not isInstance(className) then _print("Could not create ".. tostring(className)) return end if  type(values) ~= "table" then _print("\"values\" has to be a table.") return end	local newOther = Instance.new(className, parent)		for i, v in pairs (values) do 		if not isProperty(newOther, i) and not table.find(i, blacklistedValues) then _print("Could not set ".. tostring(i) .. " to " .. tostring(v)) continue end 				newOther[i] = v 	end		return newOther end 
-- UI Func > Effects
local function newParticle(color)	if typeof(color) ~= "Color3" then _print("\"color\" has to be a Color3 userdata.") return false end		local image = Instance.new("ImageLabel") other("UICorner", image, {CornerRadius = UDim.new(0,3)})		for i, v in pairs (defaultParticleProperties) do 		if not isProperty(image, i) then 			_print("Could not set ".. tostring(i) .. " to " .. tostring(v))						continue		end				image[i] = v	end	image.ImageColor3 = color	image.Rotation = math.random(0,90)	return image end


-- UI Handler
local scGui = screenGui()
local frameHandler = frame(scGui, { --ScreenGui.Handler
	Size = UDim2.new(.2,0,1,0),
	Position = UDim2.new(0.005,0,0,0),
	BackgroundTransparency = 1, 
	Name = "Handler"
})
local backgroundGlow = other("ImageLabel", frameHandler, {
	BackgroundTransparency = 1, 
	BorderSizePixel = 0,
	Position = UDim2.new(-.069, 0, .054,0),
	Size = UDim2.new(1.134,0,.89,0), 
	Image = "http://www.roblox.com/asset/?id=10272313271",
	ImageColor3 = uiColor,
})



local closeFrame = frame(frameHandler, {
	Size = UDim2.new(1.2,0,.05,0),
	Position = UDim2.new(0,0,0.1,0),
	BackgroundTransparency = 1, 
	BorderSizePixel = 0,
	Name = "CloseFrame"
})
local closeFrameSizeConstraint = other("UISizeConstraint", closeFrame, {
	MinSize = Vector2.new(180,0)
})
local closeButton = other("TextButton", closeFrame, {
	BackgroundTransparency = 1 ,
	BorderSizePixel = 0, 
	Position = UDim2.new(.65,0,0,0),
	Size = UDim2.new(.148,0,1,0),
	Text = "<b>X</b>",
	TextTransparency = 1,
	Font = Enum.Font.Cartoon,
	TextColor3 = uiColor, 
	RichText = true,
	TextScaled = true,
	Rotation = 95*3,
	Name = "Button",
})
local closeButtonSizeConstraint = other("UISizeConstraint", closeButton, {
	MinSize = Vector2.new(42,42)
})



local frameListBackground = frame(frameHandler, {
	Size = UDim2.new(1,0,.8,0),
	Position = UDim2.new(0,0,0.1,0),
	BackgroundColor3 = colors.DarkGrey,
	BackgroundTransparency = .4,
	Name = "Background",
	ClipsDescendants = true,
})
local backgroundSizeConstraint = other("UISizeConstraint", frameListBackground, {
	MinSize = Vector2.new(150,300)
})
local backgroundCorner = other("UICorner", frameListBackground, {
	CornerRadius = UDim.new(0,4)
})
local backgroundStroke = other("UIStroke", frameListBackground, {
	Color = uiColor,
	Thickness = 2,
	Transparency = .3
})
local backgroundGradient = other("UIGradient", frameListBackground, {
	Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, colors.LightGrey),
		ColorSequenceKeypoint.new(.5, colors.White),
		ColorSequenceKeypoint.new(1, colors.LightGrey)
	}),
	Rotation = 45,
})



local frameInfoHandler = frameListBackground:Clone()
editUi(frameInfoHandler, frameHandler, {
	BackgroundTransparency = 0,
	Position = UDim2.new(1.22,0,.1,0),
	Size = UDim2.new(1.5,0,.355,0),
	Name = "InfoFrame",
	ClipsDescendants = false,
	Visible = false
})
local infoGlow = backgroundGlow:Clone()
editUi(infoGlow, frameInfoHandler, {
	Position = UDim2.new(-.05,0,-.06,0),
	Size = UDim2.new(1.105,0,1.12,0),
})
local frameInfoOverHead = frame(frameInfoHandler, { -- IMPORTANT
	BackgroundColor3 = colors.DarkGrey, 
	BackgroundTransparency = .8,
	Position = UDim2.new(.1,0,.02,0),
	Size = UDim2.new(.8,0,.1,0),
	Name = "OverHead",
})
local infoOverHeadUICorner = other("UICorner", frameInfoOverHead, {
	CornerRadius = UDim.new(0,4)
})
local infoOverHeadUIStroke = other("UIStroke", frameInfoOverHead, {
	Thickness = 2, 
	Transparency = 0.3
})
local titleInfoOverHead = other("TextLabel", frameInfoOverHead, {
	BackgroundTransparency = 1, 
	BorderSizePixel = 0, 
	Position = UDim2.new(0,0,0.15,0),
	Size = UDim2.new(1,0,.7,0), 
	Font = Enum.Font.SciFi, 
	RichText = true, 
	Text = "GAME NAME(bold)",
	Name = "Title",
	TextColor3 = uiColor, 
	TextScaled = true,
	TextTransparency = .3
})
local frameInfoDesc = frame(frameInfoHandler, {
	BackgroundColor3 = colors.DarkGrey, 
	BackgroundTransparency = .8,
	Position = UDim2.new(.025,0,.16,0),
