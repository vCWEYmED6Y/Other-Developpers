	TextYAlignment = Enum.TextYAlignment.Top,
	Name = "GameDesc",
})



local frameListHandler = other("ScrollingFrame",frameListBackground, {
	Size = UDim2.new(.95,0,.78,0),
	Position = UDim2.new(0.024,0,0.04,0),
	BackgroundColor3 = colors.White,
	BackgroundTransparency = 1,
	BorderSizePixel = 0,
	AutomaticCanvasSize = Enum.AutomaticSize.Y,
	Name = "List",
	ClipsDescendants = false,
	CanvasSize = UDim2.new(0,0,0,0),
	ScrollBarThickness = 0,
})
local listlistLayout = other("UIListLayout", frameListHandler, {
	Padding = UDim.new(.01,0),
	SortOrder = Enum.SortOrder.Name,
})



local frameHandlerExample = frame(nil, {
	Size = UDim2.new(1,0,.08,0),
	BackgroundTransparency = 1, 
	BorderSizePixel = 0,
	Name = "Example",
})
local frameBackgroundExample = frame(frameHandlerExample, {
	BackgroundColor3 = colors.DarkGrey,
	BackgroundTransparency = .8,
	Size = UDim2.new(1,0,1,0), 
	Name = "Background"
})
local exampleCorner = other("UICorner", frameBackgroundExample, {
	CornerRadius = UDim.new(0,3)
})
local exampleStroke = other("UIStroke", frameBackgroundExample, {
	ApplyStrokeMode = Enum.ApplyStrokeMode.Border,
	Color = Color3.fromRGB(10,10,10), 
	Thickness = 2, 
	Transparency = .2, 
})
local frameHover = frame(frameHandlerExample, {
	BackgroundColor3 = colors.White,
	BackgroundTransparency = 1,
	Size = UDim2.new(1,0,1,0),
	ZIndex = 99,
	Name = "Hover",
})
local hoverCorner = other("UICorner", frameHover, {
	CornerRadius = UDim.new(0,3),
})
local exampleButton = other("TextButton", frameHandlerExample, {
	Size = UDim2.new(1,0,.6,0),
	Position = UDim2.new(0,0,.2,0),
	TextColor3 = colors.White,
	BackgroundTransparency = 1,
	BorderSizePixel = 0, 
	ZIndex = 9999,
	Font = Enum.Font.SciFi,
	RichText = true,
	TextScaled = true,
	Text = "",
	TextTransparency = .3,
	Name = "Button"
})
local exampleSizeConstraint = other("UISizeConstraint", frameHandlerExample, {
	MinSize = Vector2.new(0,40)
})



while true do 
	for i, v in pairs (pages:GetCurrentPage()) do
		local currentNumber = addedValue+i
		local exampleCopy = frameHandlerExample:Clone()
		exampleCopy.Parent = frameListHandler
		exampleCopy.Name = currentNumber
		if v.PlaceId == game.PlaceId then 
			exampleCopy.Button.Text = "<font color = 'rgb("..stringColor..")'>[</font><font color = 'rgb(255,255,255)'>".. v.Name.."</font><font color = 'rgb("..stringColor..")'>]</font>" -- Current Game
			oldNames[currentNumber] = "<font color = 'rgb("..stringColor..")'>[</font><font color = 'rgb(255,255,255)'>".. v.Name.."</font><font color = 'rgb("..stringColor..")'>]</font>"
			currentText[currentNumber] = oldNames[currentNumber]
		else
			exampleCopy.Button.Text = "<font color = 'rgb(255,255,255)'>".. v.Name.."</font>" -- Not Game
			oldNames[currentNumber] = "<font color = 'rgb(255,255,255)'>".. v.Name.."</font>"
			currentText[currentNumber] = oldNames[currentNumber]
		end
		exampleCopy.MouseEnter:Connect(function()
			tService:Create(exampleCopy.Hover, TweenInfo.new(.2, Enum.EasingStyle.Quad), {BackgroundTransparency = .89}):Play()
		end)

		exampleCopy.MouseLeave:Connect(function()
			tService:Create(exampleCopy.Hover, TweenInfo.new(.2, Enum.EasingStyle.Quad), {BackgroundTransparency = 1}):Play()
		end)

		lastClicked["ClipBoard"][currentNumber] = 0
		lastClicked["Teleport"][currentNumber] = 0
		index[v.PlaceId] = currentNumber
		
		if getrenv().GameInfos then
			gameDataStore[currentNumber] = {}
			local currentDataStore = gameDataStore[currentNumber]
			local data = getPlaceDetails(v.PlaceId)
			currentDataStore["Name"] = v.Name
			currentDataStore["Data"] = data 
			currentDataStore["LastUpdate"] = tick()
			
			exampleCopy.MouseEnter:Connect(function()
			    left = false
				frameInfoHandler.Visible = true 
				titleInfoOverHead.Text = "<b>".. currentDataStore["Name"] .."</b>"
				task.spawn(spawn,function()
					if particlesEnabled then 
						for i = 1, particleAmount*2 do 
							local image = newParticle(uiColor)

							if image then 
								image.Parent = frameInfoHandler
								image.Position = UDim2.new(math.random(0,1000)/1000,0,1,0)

								particleModule.createParticle(
									image,
									100, 
									UDim2.new(math.random(-1,1)/1000,0,.0005,0),
									math.random(-5,-5), 
									UDim2.new(0,0,0.0002,0),
									UDim2.new(-0.001,0,-0.001,0),
									0.01,
									0,
									frameInfoHandler
								)
							end
						end
					end
				end)
				if currentDataStore["Data"] then 
					local isGroup = (data.BuilderAbsoluteUrl:match("groups") == "groups")
					
					playerDescription.Text = "<b><font color = 'rgb("..stringColor..")'>Username: </font></b>".. data.Builder .."\n<b><font color = 'rgb("..stringColor..")'>UserID: </font></b>".. tostring(data.BuilderId) .."\n<b><font color = 'rgb("..stringColor..")'>Group: </font></b>".. tostring(isGroup)
					gameDescription.Text = "<b><font color = 'rgb("..stringColor..")'>Description: </font></b>\"".. data.Description .. "\"\n<b><font color = 'rgb("..stringColor..")'>Created: </font></b>".. data.Created .."\n<b><font color = 'rgb("..stringColor..")'>Updated: </font></b>".. data.Updated .."\n<b><font color = 'rgb("..stringColor..")'>Favorites: </font></b>".. tostring(data.FavoritedCount) .."\n<b><font color = 'rgb("..stringColor..")'>Visits: </font></b>".. tostring(data.VisitedCount) .."\n<b><font color = 'rgb("..stringColor..")'>Online: </font></b>".. tostring(data.OnlineCount)
					
					if isGroup then 
						playerImage.Image = getGroupInfos(data.BuilderId).EmblemUrl
					else 
						playerImage.Image = game.Players:GetUserThumbnailAsync(data.BuilderId, "HeadShot", "Size60x60")
					end
				end
			end)
		
			exampleCopy.MouseLeave:Connect(function()
				left = true
				delay(.5, function()
				    if left then 
				        frameInfoHandler.Visible = false
				    end 
				end)
			end)
		end
		
		exampleCopy.Button.MouseButton1Click:Connect(function()
			if getrenv().MobileFix then 
				print(v.PlaceId)
			end
			task.spawn(spawn,function()
				if particlesEnabled then 
					for i = 1, particleAmount do 
						local image = newParticle(colors.DarkGrey)

						if image then 
							image.Position = UDim2.new(math.random(0,1000)/1000,0,1.05,0)

							particleModule.createParticle(
								image,
								50, 
								UDim2.new(math.random(-1,1)/1000,0,.0005,0),
								math.random(-5,-5), 
								UDim2.new(0,0,0.002,0),
								UDim2.new(-0.001,0,-0.001,0),
								0.01,
								0,
								exampleCopy.Button
							)
						end
					end
				end
			end)
			if not isTeleporting then 
				lastClicked["Teleport"][currentNumber] = tick()
				isTeleporting = true
				pcall(function()
					tpService:Teleport(v.PlaceId, game.Players.LocalPlayer)
				end)
				repeat task.wait()
					exampleCopy.Button.Text = currentText[currentNumber] .. "<font color = 'rgb("..stringColor..")'> ‚Üê [".. math.floor((tick()-lastClicked["Teleport"][currentNumber])*10 )/10 .."]</font>"
				until (tick() -lastClicked["Teleport"][currentNumber]) >= teleportAbortDelay or teleportFailure[v.PlaceId]
				if teleportFailure[v.PlaceId] then 
					exampleCopy.Button.Text = oldNames[currentNumber]
					teleportFailure[currentNumber] = nil
				end
				isTeleporting = false 
			end
		end)

		exampleCopy.Button.MouseButton2Click:Connect(function()
			setclipboard(tostring(v.PlaceId))
			exampleCopy.Button.Text = "<font color = 'rgb("..stringColor..")'>".. v.PlaceId .."</font>"
			currentText[currentNumber] = "<font color = 'rgb("..stringColor..")'>".. v.PlaceId .."</font>"
			lastClicked["ClipBoard"][currentNumber] = tick()
			task.spawn(spawn,function()
				if particlesEnabled then 
					for i = 1, particleAmount do 
						local image = newParticle(uiColor)
						
						if image then 
							image.Parent = exampleCopy.Button
							image.Position = UDim2.new(math.random(0,1000)/1000,0,1.05,0)
							
							particleModule.createParticle(
								image,
								50, 
								UDim2.new(math.random(-1,1)/1000,0,.0005,0),
								math.random(-5,-5), 
								UDim2.new(0,0,0.002,0),
								UDim2.new(-0.001,0,-0.001,0),
								0.01,
								0,
								exampleCopy.Button
							)
						end
					end
				end
			end)
			delay(1.5, function()
				if (tick() - lastClicked["ClipBoard"][currentNumber]) >= 1.4 then
					exampleCopy.Button.Text = oldNames[currentNumber]
					currentText[currentNumber] = oldNames[currentNumber]
				end
			end)
		end)
	end
	if pages.IsFinished then 

		break
	end
	pages:AdvanceToNextPageAsync()
	addedValue = addedValue + 50
end

scGui.Enabled = true

-- Other Events
frameListBackground.MouseEnter:Connect(function()
	tService:Create(frameListBackground, TweenInfo.new(.185, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
end)
frameListBackground.MouseLeave:Connect(function()
	tService:Create(frameListBackground, TweenInfo.new(.185, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0.4}):Play()
end)

closeFrame.MouseEnter:Connect(function()
	tService:Create(closeButton, TweenInfo.new(.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Position = UDim2.new(.85,0,0,0), Rotation = 0, TextTransparency = 0}):Play()
end)
closeFrame.MouseLeave:Connect(function()
	tService:Create(closeButton, TweenInfo.new(.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Position = UDim2.new(.65,0,0,0), Rotation = 90*3, TextTransparency = 1}):Play()
end)
closeButton.MouseButton1Click:Connect(function()
	getrenv().ScreenGui:Destroy()
	getrenv().ScreenGui = nil 
	getrenv().UniverseViewerDestroy = nil
	getrenv().UniverseViewerExecuted = false
end)

-- Hooks 
tpService.TeleportInitFailed:Connect(function(player, result, err, place)
	teleportFailure[place] = true
end)

-- Global Func 
getrenv().UniverseViewerDestroy = function()
	getrenv().ScreenGui:Destroy()
	getrenv().ScreenGui = nil 
	getrenv().UniverseViewerDestroy = nil
	getrenv().UniverseViewerExecuted = false
end 
